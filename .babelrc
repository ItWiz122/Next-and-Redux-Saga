{
  "presets": [["env", {"modules": false}], "react", "stage-3"],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [["env", {"modules": "commonjs"}]]
    }
  }
}
